{
  "name": "odyssey-runner",
  "version": "1.0.0",
  "description": "Odyssey Runner Service - Background workers for data processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/runners/sync.js",
    "dev": "tsx watch src/runners/sync.ts",
    "sync": "tsx src/runners/sync.ts --",
    "sync:dev": "tsx watch src/runners/sync.ts",
    "sync:prod": "node dist/runners/sync.js",
    "outbound": "tsx src/runners/outbound.ts --",
    "outbound:dev": "tsx watch src/runners/outbound.ts",
    "outbound:prod": "node dist/runners/outbound.js",
    "partitions": "tsx src/runners/partitions.ts --",
    "partitions:dev": "tsx watch src/runners/partitions.ts",
    "partitions:prod": "node dist/runners/partitions.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "stream",
    "runner",
    "pgmq",
    "supabase",
    "worker",
    "partition",
    "etl"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@duckdb/node-api": "^1.4.2-r.1",
    "@supabase/supabase-js": "^2.49.1",
    "dotenv": "^16.4.7",
    "mysql2": "^3.14.0",
    "ssh2": "^1.16.0",
    "ssh2-sftp-client": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/ssh2": "^1.15.1",
    "@types/ssh2-sftp-client": "^9.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
